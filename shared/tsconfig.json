{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
	"declaration": true,
	"outDir": "./build",
    "strict": true,
    "skipLibCheck": true,
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true,
    "lib": [
      "ESNext"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
}